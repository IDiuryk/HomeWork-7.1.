const text = "Wonderful, Joyful, Happiness, Time, Task, Apple";

// Регулярний вираз для пошуку слів з 6 або більше символів, котрі не містять літери "A" або "a"
const regex = /\b[^aA\s]{6,}\b/g;

// Пошук за текстом за допомогою регулярного виразу
const result = text.match(regex);

console.log(result); // Виведе: ["Wonderful", "Joyful"]
